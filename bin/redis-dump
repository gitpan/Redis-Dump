#!/usr/bin/perl

package Redis::Runner;

use strict;
use Redis::Dump;
use JSON;

our $VERSION = '0.002'; # VERSION

my $app = Redis::Dump->new_with_options();

print to_json( { $app->run }, { ascii => 1, pretty => 1 } );


__END__
